    </cac:SecurityClearanceTerm>
  </cac:TenderingTerms>
</cac:ProcurementProjectLot>
{
  "tender": {
    "lots": [
      {
        "id": "LOT-0001",
        "otherRequirements": {
          "securityClearance": "EU Confidential security clearance of Key Management Personnel must be achieved before access to procurement documents be granted"
        }
      }
    ]
  }
}
