      </cac:AwardingCriterion>
    </cac:AwardingTerms>
  </cac:TenderingTerms>
</cac:ProcurementProjectLot>
{
  "tender": {
    "lots": [
      {
        "id": "LOT-0001",
        "awardCriteria": {
          "criteria": [
            {
              "name": "Technical merit"
            }
          ]
        }
      }
    ]
  }
}
