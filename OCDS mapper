  "tender": {
    "lots": [
      {
        "id": "LOT-0001",
        "milestones": [
          {
            "id": "1",
            "type": "securityClearanceDeadline",
            "dueDate": "2019-11-15T23:59:59+01:00"
          }
        ]
      }
    ]
  }
