      </cac:Prize>
    </cac:AwardingTerms>
  </cac:TenderingTerms>
</cac:ProcurementProjectLot>
{
  "tender": {
    "lots": [
      {
        "id": "LOT-0001",
        "designContest": {
          "prizes": {
            "details": [
              {
                "id": "0",
                "value": {
                  "amount": 5000,
                  "currency": "EUR"
                }
              }
            ]
          }
        }
      }
    ]
  }
}
