<efac:Organization>
  <efac:Company>
    <cac:PartyIdentification>
      <cbc:ID schemeName="organization">ORG-0003</cbc:ID>
    </cac:PartyIdentification>
    <cac:Contact>
      <cbc:Telefax>(+33) 2 34 56 78 91</cbc:Telefax>
    </cac:Contact>
  </efac:Company>
</efac:Organization>
{
  "parties": [
    {
      "id": "ORG-0003",
      "contactPoint": {
        "faxNumber": "(+33) 2 34 56 78 91"
      }
    }
  ]
}
