<cac:ProcurementProjectLot>
  <cbc:ID schemeName="Lot">LOT-0001</cbc:ID>
  <cac:ProcurementProject>
    <cac:RequestedTenderTotal>
      <cbc:EstimatedOverallContractAmount currencyID="EUR">250000</cbc:EstimatedOverallContractAmount>
    </cac:RequestedTenderTotal>
  </cac:ProcurementProject>
</cac:ProcurementProjectLot>
{
  "tender": {
    "lots": [
      {
        "id": "LOT-0001",
        "value": {
          "amount": 250000,
          "currency": "EUR"
        }
      }
    ]
  }
}
