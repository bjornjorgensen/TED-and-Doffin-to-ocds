    <cac:PostAwardProcess>
      <cbc:ElectronicPaymentUsageIndicator>true</cbc:ElectronicPaymentUsageIndicator>
    </cac:PostAwardProcess>
  </cac:TenderingTerms>
</cac:ProcurementProjectLot>
{
  "tender": {
    "lots": [
      {
        "id": "LOT-0001",
        "contractTerms": {
          "hasElectronicPayment": true
        }
      }
    ]
  }
}
