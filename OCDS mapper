    <cbc:HigherTenderAmount currencyID="EUR">456</cbc:HigherTenderAmount>
    <efac:TenderLot>
      <cbc:ID schemeName="Lot">LOT-0001</cbc:ID>
    </efac:TenderLot>
  </efac:LotResult>
</efac:NoticeResult>
{
  "bids": {
    "statistics": [
      {
        "id": "1",
        "measure": "highestValidBidValue",
        "value": 456,
        "currency": "EUR",
        "relatedLot": "LOT-0001"
      }
    ]
  }
}
BT-712(a)-LotResult 
