  "tender": {
    "items": [
      {
        "id": "1",
        "deliveryAddresses": [
          {
            "streetAddress": "Main Street, 2, Building B1, 3rd floor"
          }
        ],
        "relatedLot": "LOT-0001"
      }
    ]
  }
}
